{
  "name": "blog",
  "author": {
    "email": "tregan@tregan.me",
    "name": "Tregan",
    "url": "https://tregan.me"
  },
  "private": true,
  "scripts": {
    "dev": "next dev | pino-pretty",
    "build": "next build --no-lint",
    "build:tsc": "tsc -m commonjs && tsc-alias",
    "start": "next start",
    "prepare": "husky install",
    "test": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn cypress open'",
    "analyze": "ANALYZE=true next build",
    "lint": "concurrently yarn:lint:*",
    "lint:code": "next lint",
    "lint:format": "prettier -c .",
    "lint:spelling": "cspell \"**/*\" --gitignore",
    "lint:style": "stylelint **/*.{scss,css}",
    "seed:up": "ts-node -O '{\"module\":\"commonjs\"}' src/scripts/seedUp",
    "seed:down": "ts-node -O '{\"module\":\"commonjs\"}' src/scripts/seedDown",
    "seed:create": "cd src/migrations && typeorm migration:create",
    "seed:gen": "yarn build:tsc && cd src/migrations && typeorm migration:generate foo -d ../../build/src/utils/dataSource.js",
    "locale:gen": "i18next 'src/**/*' -c i18next-parser.config.ts"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.2.4",
    "@next/font": "^13.2.4",
    "@types/cookie": "^0.5.1",
    "@types/mapbox-gl": "^2.7.6",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.11",
    "@types/request-ip": "^0.0.37",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.1",
    "@types/xml": "^1.0.8",
    "aws-sdk": "^2.1353.0",
    "axios": "^1.3.4",
    "celebrate": "^15.0.1",
    "class-transformer": "^0.5.1",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.14.0",
    "classnames": "^2.3.2",
    "cookie": "^0.5.0",
    "exifr": "^7.1.3",
    "geojson": "^0.5.0",
    "i18next-parser": "^7.7.0",
    "jose": "^4.10.0",
    "mapbox-gl": "^2.10.0",
    "next": "^13.2.4",
    "next-axiom": "^0.11.1",
    "next-connect": "^0.13.0",
    "next-i18next": "^12.1.0",
    "pg": "^8.10.0",
    "pino": "^8.7.0",
    "pino-http": "^8.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "sharp": "^0.32.0",
    "swr": "^2.1.1",
    "typeorm": "^0.3.12",
    "ulid": "^2.3.0",
    "uuid": "^9.0.0",
    "xml": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@faker-js/faker": "^7.5.0",
    "@svgr/webpack": "^7.0.0",
    "@types/node": "^17.0.8",
    "concurrently": "^7.4.0",
    "cspell": "^6.12.0",
    "cypress": "^10.7.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.26.0",
    "eslint-config-treemmett": "^0.5.2",
    "husky": "^7.0.4",
    "lint-staged": "^13.2.1",
    "pino-pretty": "^9.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^15.3.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard-scss": "^7.0.1",
