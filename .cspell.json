{
  "version": "0.2",
  "import": [
    "@cspell/dict-de-de/cspell-ext.json",
    "@cspell/dict-et-ee/cspell-ext.json",
    "@cspell/dict-fr-fr/cspell-ext.json"
  ],
  "language": "en",
  "dictionaries": ["typescript", "html"],
  "words": [
    "Batang",
    "chainable",
    "commitlint",
    "componentize",
    "csstree",
    "Gowun",
    "heic",
    "heif",
    "jimp",
    "mapbox",
    "mapboxgl",
    "minio",
    "picsum",
    "preconnect",
    "preload",
    "stylelint",
    "svgr",
    "testid",
    "treemmett",
    "Tregan",
    "typeorm",
    "ulid",
    "vercel"
  ],
  "overrides": [
    {
      "filename": "public/locales/de/**",
      "language": "de,en-US"
    },
    {
      "filename": "public/locales/et/**",
      "language": "et,en-US"
    },
    {
      "filename": "public/locales/fr/**",
      "language": "fr,en-US"
    },
    {
      "filename": "**/*.svg",
      "languageId": "css,html"
    }
  ]
}
